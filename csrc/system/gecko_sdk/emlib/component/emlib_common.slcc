id: emlib_common
label: Common Headers
package: platform
description: >
  Common headers for EMLIB, providing atomic bit access, macros for alignment,
  min/max/ceil/floor, ramfunc, pack, section, weak, noreturn, fallthrough and versioning.
category: Platform|Peripheral
quality: production
component_root_path: platform/emlib
include:
  - path: "inc"
    file_list:
    - path: "em_assert.h"
    - path: "em_bus.h"
    - path: "em_common.h"
    - path: "em_ramfunc.h"
    - path: "em_version.h"
provides:
  - name: "emlib_common"
requires:
  - name: "cmsis_core"
  - name: "device"
  - name: "sl_common"
  - name: "sl_assert"
documentation:
  docset: gecko-platform
  document:
